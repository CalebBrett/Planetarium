{
	"builds": [
		{
			"src": "api/index.py",
			"use": "@vercel/python",
			"config": { "runtime": "python3.9" }
		},
		{
			"src": "client/package.json",
			"use": "@vercel/static-build"
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "/api/index.py"
		},
		{
			"src": "/(.*)",
			"dest": "/client/$1"
		}
	],
	"installCommand": "npm install",
	"buildCommand": "npm run build",
	"outputDirectory": "client/"
}
